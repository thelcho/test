<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:text="hook" />

    <TextView
        android:id="@+id/tv_custom_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="custom_view" />

    <TextView
        android:id="@+id/tv_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Service" />

    <TextView
        android:id="@+id/tv_broadcast"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Broadcast" />

    <TextView
        android:id="@+id/tv_content_resolver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="content_resolver" />
    <TextView
        android:id="@+id/tv_database"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="database" />
    <TextView
        android:id="@+id/tv_light_sensor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LightSensor" />

</LinearLayout>